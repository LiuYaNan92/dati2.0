page{
  background:#fff;
}
.head_img{
  width:100%;
  height:242rpx;
  display:flex;
  justify-content: center;
  align-items: center;
}
.head_img .img{
  width:188rpx;
  height:188rpx;
  border-radius:50%;
  overflow: hidden;
}
.head_img image{
  width:188rpx;
  height:188rpx;
}
.list_box{
  margin-top:30rpx;
  border-top:2rpx solid #f6f6f6;
}
.list_box .list{
  height:100rpx;
  line-height:100rpx;
  margin-left:36rpx;
  border-bottom:2rpx solid #f6f6f6;
  display:flex;
  align-items: center;
}
.list_box .list .text1{
  font-size:28rpx;
  width:120rpx;
}
.list_box .list .text2{
  font-size:26rpx;
  color:#b1b1b1;
  margin-left:69rpx;
}
.list_box .list input{
  margin-left:69rpx;
  height:80rpx;
  font-size:26rpx;

}
.saveBtn{
  background:none;
  width:620rpx;
  height: 88rpx;
  color:#fff;
  background:#75b2b7!important;
  border-radius:44rpx;
  font-weight: normal;
  line-height:88rpx;
  text-align: center;
  margin:50rpx auto;
}